import React from 'react';
import PropTypes from 'prop-types';
import { Component } from 'react';
import './card.css';


class Card extends Component {

    render() {

    
            return (
                <li className="cards-this.propss" key={this.props.id}>
                    <img src={this.props.this.props.imageUrl} alt="dog" />
                    <div className="menu__this.props-text">
                        <div className="menu__this.props-subtitle">{this.props.name}</div>
                        <div className="menu__this.props-descr">{this.props.temperament}</div>
                    </div>
                </li>
            )
    }
}
Card.PropTypes = {
    id: PropTypes.number,
    imageUrl: PropTypes.string,
    name: PropTypes.string,
    temperament: PropTypes.string,
}
export default Card;